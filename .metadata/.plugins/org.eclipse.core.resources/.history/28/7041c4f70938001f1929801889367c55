public class ArrayTest {

	public static void main(String[] args) {
		
		// 배열
		int[] arr1 = {1, 2, 3, 4, 5};
		char[] arr2 = {'A', 'B', 'C'};
		String[] arr3 = {"서울", "대전", "대구", "부산", "광주"};
		
		// 배열 원소 : 배열의 인덱스로 배열 값 참조
		System.out.println("arr1 1번째 원소 : " + arr1[0]);
		System.out.println("arr1 2번째 원소 : " + arr1[1]);
		System.out.println("arr1 3번째 원소 : " + arr1[2]);

		System.out.println("arr1 1번째 원소 : " + arr1[0]);
		System.out.println("arr1 3번째 원소 : " + arr1[2]);

		System.out.println("arr1 1번째 원소 : " + arr1[0]);
		System.out.println("arr1 4번째 원소 : " + arr1[3]);
		System.out.println("arr1 5번째 원소 : " + arr1[4]);

		// 배열 길이
		
		
		// 배열 반복문
		
		// 1차원 배열
		
		// 2차원 배열
		
		// 3차원 배열
		
	}
}
