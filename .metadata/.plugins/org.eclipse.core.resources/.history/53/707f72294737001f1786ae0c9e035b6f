package sub3;

/*
 * 날짜 : 2024/07/01
 * 이름 : 김소희
 * 내용 : 반복문 for 실습하기 
 */
public class ForTest {
	public static void main(String[] args) {
		
		// for
		for( int i=1 ; i<=10 ; i++ ) {
			
			System.out.println("i : " + i);
			
		}
		// 1부터 10까지 합
		// 1부터 10까지 짝수합
		// for 중첩
		// 구구단
		// 별삼각형
		
	}

}
