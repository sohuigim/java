package test1;

import java.util.Scanner;

/*
 * 날짜 : 2024/07/03
 * 이름 : 김소희
 * 내용 : 메서드 오버로딩 실습하기
 */

public class Test08 {
	public static void main(String[] args) {
		
		int n = 5;
		
		for(int i = 0; i<n ; i++) {
			
			for(int j=n-1 ; ; j--) {
				System.out.print(" ");
			}
			
			for(int j=0 ; ; j++) {
				System.out.print("*");
			}
			
			System.out.println("\n");
		}
	}
}
