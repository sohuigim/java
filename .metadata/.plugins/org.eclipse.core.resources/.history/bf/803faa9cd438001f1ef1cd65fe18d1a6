package test1;

public class T1 {

	package test1; public class Test01 { public static void main(String[] args) { char var1 = 'A'; // 문자형int var2 = 100; // 정수형long var3 = 2147483648L; // 큰 정수형 (long 타입)float var4 = 3.14f; // 실수형 (float 타입)double var5 = 3.14159; // 실수형 (double 타입)boolean var6 = true; // 불리언형 (참)boolean var7 = false; // 불리언형 (거짓)String var8 = "hello"; // 문자열형Object var9 = null; // null 객체 System.out.println("var1 = " + var1); System.out.println("var2 = " + var2); System.out.println("var3 = " + var3); System.out.println("var4 = " + var4); System.out.println("var5 = " + var5); System.out.println("var6 = " + var6); System.out.println("var7 = " + var7); System.out.println("var8 = " + var8); System.out.println("var9 = " + var9); } }



	package test1; public class Test01 { public static void main(String[] args) { char var1 = 'A'; // 문자형int var2 = 100; // 정수형long var3 = 2147483648L; // 큰 정수형 (long 타입)float var4 = 3.14f; // 실수형 (float 타입)double var5 = 3.14159; // 실수형 (double 타입)boolean var6 = true; // 불리언형 (참)boolean var7 = false; // 불리언형 (거짓)String var8 = "hello"; // 문자열형Object var9 = null; // null 객체 System.out.println("var1 = " + var1); System.out.println("var2 = " + var2); System.out.println("var3 = " + var3); System.out.println("var4 = " + var4); System.out.println("var5 = " + var5); System.out.println("var6 = " + var6); System.out.println("var7 = " + var7); System.out.println("var8 = " + var8); System.out.println("var9 = " + var9); } }




}
